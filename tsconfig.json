{
	"compilerOptions": {
		"baseUrl": "./src/ts",
		"module": "ES6",
		"sourceMap": true,
		"moduleResolution": "node",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES2015",
			"ScriptHost"
		],
		"esModuleInterop": true,
		"typeRoots": ["./src/ts/types", "./node_modules/@types"]
	},
	"include": [
		"src/ts/*.ts",
	]
}
